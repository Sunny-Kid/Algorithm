{
  "typescript.validate.enable": true,
  "javascript.validate.enable": false,
  "editor.formatOnSave": true,
  "tslint.enable": true,
  "npm.updateStrategy": "SEMVER",
  "html.format.enable": false,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.options": {
    "extensions": [
      ".js",
      ".vue",
      ".ts",
      ".tsx"
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "npm.registry": "https://registry.npm.taobao.org",
  "files.associations": {
    "*.ttml": "xml",
    "*.ttss": "css"
  }
}