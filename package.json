{
  "name": "js-data-structures",
  "version": "1.0.0",
  "description": "learning algorithm and commonly-used data-structures lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-Kid/Algorithm.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "lint:fix": "eslint src --fix",
    "build": "rollup -c",
    "build:dev": "NODE_ENV=development webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack --progress --colors"
  },
  "author": "Hank Ren",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "lint-staged": {
    "src/**/*.js": "eslint --max-warnings=1 --ignore-pattern !.umirc.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "sideEffects": false,
  "dependencies": {}
}
